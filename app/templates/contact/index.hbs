<h2>All Contacts</h2>
<h3>{{#link-to "contact.new"}}Add New Contact{{/link-to}}</h3>

<ul>
  {{#each model as |contact|}}
    <li>
      {{#link-to "contact.detail" contact}}{{contact.lastName}}, {{contact.firstName}}{{/link-to}}
      <button onclick={{action "remove" contact}}>Delete</button>
    </li>
  {{/each}}
</ul>
